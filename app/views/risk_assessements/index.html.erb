<h1>Listing risk_assessements</h1>

<table>
  <thead>
    <tr>
      <th>Probability risk</th>
      <th>Consequense risk</th>
      <th>Risk level</th>
      <th>Task</th>
      <th>Safety report</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @risk_assessements.each do |risk_assessement| %>
      <tr>
        <td><%= risk_assessement.probability_risk %></td>
        <td><%= risk_assessement.consequense_risk %></td>
        <td><%= risk_assessement.risk_level %></td>
        <td><%= risk_assessement.task %></td>
        <td><%= risk_assessement.safety_report %></td>
        <td><%= link_to 'Show', risk_assessement %></td>
        <td><%= link_to 'Edit', edit_risk_assessement_path(risk_assessement) %></td>
        <td><%= link_to 'Destroy', risk_assessement, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Risk assessement', new_risk_assessement_path %>
